{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": true,
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        }
      ]
    },
    "layout": {
      "contentSize": "700px",
      "wideSize": "1200px"
    },
    "spacing": {
      "defaultSpacingSizes": false
    },
    "typography": {
      "writingMode": false
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "spacing": {
      "padding": {
        "left": "var:preset|spacing|50",
        "right": "var:preset|spacing|50"
      }
    },
    "blocks": {
      "core/group": {
        "spacing": {
          "margin": {
            "top": "0px"
          }
        }
      }
    }
  },
  "customTemplates": [],
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "patterns": []
}